{
  "name": "eoapi",
  "version": "0.0.1",
  "description": "API ToolKit",
  "homepage": "https://github.com/eolinker/eoapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/eolinker/eoapi"
  },
  "author": {
    "name": "eoapi",
    "email": "dev@eolink.com"
  },
  "keywords": [
    "angular",
    "angular 13",
    "electron",
    "electron 16",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "(cd app && npm install)&&electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "ng:web": "ng serve -c webaot -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "web:build:": "npm run electron:serve-tsc && ng build -c production",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:web": "npx playwright test e2e/cases/",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "wininstaller": "cd build && build-nim-nozip.bat",
    "electron:build:win": "npm run electron:build && npm run wininstaller",
    "release": "electron-builder"
  },
  "dependencies": {
    "@angular/animations": "13.2.0",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/language-service": "13.2.0",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/router": "13.2.0",
    "@angular/upgrade": "^13.2.0",
    "@ngxs/store": "3.7.3",
    "angular": "1.8.2",
    "dexie": "3.2.0",
    "js-beautify": "1.14.0",
    "ng-zorro-antd": "13.0.1",
    "ngx-ace-wrapper": "11.0.1",
    "rxjs": "7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.0.0",
    "@angular-devkit/build-angular": "13.2.0",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.0",
    "@angular/compiler-cli": "13.2.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@playwright/test": "1.18.1",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "conventional-changelog-cli": "2.2.2",
    "electron": "16.0.8",
    "electron-builder": "22.14.5",
    "electron-reload": "1.5.0",
    "eslint": "8.8.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.7.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.12",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.1.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lodash": "4.17.21",
    "node-polyfill-webpack-plugin": "1.1.4",
    "npm-run-all": "4.1.5",
    "ts-node": "10.4.0",
    "typescript": "~4.5.5",
    "wait-on": "6.0.0",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome 91"
  ],
  "__npminstall_done": false
}
