{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "__crontab": {
      "type": "number",
      "label": "Auto Sync",
      "ui:widget": "radio",
      "default": 4,
      "required": true,
      "oneOf": [
        {
          "type": "number",
          "title": "Off",
          "default": 0,
          "const": 0
        },
        {
          "type": "number",
          "title": "Every four hours",
          "default": 4,
          "const": 4
        },
        {
          "type": "number",
          "title": "Every twelve hours",
          "default": 12,
          "const": 12
        },
        {
          "type": "number",
          "title": "Every day",
          "default": 24,
          "const": 24
        }
      ]
    },
    "__formater": {
      "type": "string",
      "label": "Format",
      "ui:widget": "radio",
      "default": "foo",
      "required": true,
      "oneOf": []
    }
  },
  "allOf": []
}
