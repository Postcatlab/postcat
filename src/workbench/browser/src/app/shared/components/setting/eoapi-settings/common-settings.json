{
  "private": true,
  "name": "eoapi-common",
  "version": "0.0.1",
  "moduleName": "通用",
  "moduleID": "eoapi-common",
  "author": "eoapi",
  "publisher": "eoapi",
  "features": {
    "configuration": {
      "type": "object",
      "title": "通用",
      "order": 1,
      "properties": {
        "common.app.autoUpdate": {
          "type": "boolean",
          "default": true,
          "label": "自动升级",
          "disabled": true,
          "description": "检测到应用升级后，后台将下载最新的安装包在下次启动时更新"
        },
        "remoteServer.url": {
          "type": "string",
          "label": "远程服务器地址",
          "required": true,
          "default": "http://localhost:3000",
          "placeholder": "数据源地址",
          "description": "从远程服务器存取数据，配置后支持多人合作，配置文档请见：[远程数据源](https://eoapi.io/docs/storage.html)"
        },
        "remoteServer.token": {
          "type": "string",
          "label": "Token",
          "required": true,
          "default": "1ab2c3d4e5f61ab2c3d4e5f6",
          "placeholder": "Token 凭证",
          "description": "远程服务器 Token，配置后才可访问"
        }
      }
    }
  }
}
