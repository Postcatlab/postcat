{
  "private": true,
  "name": "eoapi-features",
  "version": "0.0.1",
  "moduleName": "功能",
  "moduleID": "eoapi-features",
  "author": "eoapi",
  "publisher": "eoapi",
  "features": {
    "configuration": [
      {
        "type": "object",
        "title": "API测试",
        "order": 1,
        "properties": {
          "api.test": {
            "type": "boolean",
            "default": false,
            "label": "自动重定向",
            "description": "自动重定向至redirect url"
          }
        }
      },
      {
        "type": "object",
        "title": "Mock",
        "order": 1,
        "properties": {
          "mock.matchType": {
            "type": "boolean",
            "default": false,
            "label": "匹配请求方式",
            "description": " 开启后，系统会匹配和 API 文档请求方式（GET、POST...）一致的 Mock"
          }
        }
      },
      {
        "type": "object",
        "title": "扩展",
        "order": 2,
        "properties": {
          "extension.autoupdate": {
            "type": "boolean",
            "default": true,
            "label": "自动更新",
            "description": "勾选后，检测到插件升级后自动从远程下载最新的插件更新"
          }
        }
      }
    ]
  }
}
