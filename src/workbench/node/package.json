  "dependencies": {
    "@koa/cors": "3.3.0",
    "crypto-js": "4.1.1",
    "jquery": "3.6.1",
    "jsdom": "20.0.1",
    "koa": "2.13.4",
    "koa-body": "5.0.0",
    "pm2": "5.2.0",
    "socket.io": "4.5.3",
    "xml2js": "0.4.23"
  }
}
