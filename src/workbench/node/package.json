{
  "name": "eo-test-server",
  "version": "1.0.0",
  "description": "Recieve eoapi tes config and response http result",
  "main": "./server/main.js",
  "scripts": {
    "dev": "node ./server/main.js",
    "start": "pm2-runtime start ./server/main.js",
    "start:io": "pm2-runtime start ./server/socketio.js",
